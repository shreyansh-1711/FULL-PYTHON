punctuations = '''!()-[]{};:'"\,<>./?@#$%^&*_~'''

my_str = "Hello!!!, he said ---and went."

no_punct = ""
patch-2
for char in my_str:
   if char not in punctuations:
       no_punct = no_punct + char

for char in my_str:

if char not in punctuations:

no_punct = no_punct + char

master
